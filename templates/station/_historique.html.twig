{% if station.airQualityData|length > 0 %}
    <table class="table table-bordered">
        <thead>
        <tr>
            <th>Data</th>
        </tr>
        </thead>
        {% for airQualityData in station.airQualityData %}
            <tr>
                <td>
                    {{ dump(airQualityData) }}
                </td>
            </tr>
        {% endfor %}
    </table>
{% else %}
    <p class="text-warning">Aucun indice
        trouvé {% if date is defined %} à la date {{ date|date('d-m-Y') }} {% endif %}</p>
{% endif %}